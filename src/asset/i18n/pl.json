          }
        }
      },
      "/api/v1/event/{id}/done": {
        "after": {
          "success": {
            "header": "Wydarzenie zakończone",
            "message": "Wydarzenie zostało pomyślnie zakończone"
          }
        }
      },
      "/api/v1/event/{id}/rejected": {
        "after": {
          "success": {
            "header": "Wydarzenie odrzucone",
            "message": "Wydarzenie zostało pomyślnie odrzucone"
          }
        }
      },
      "/api/v1/event/{id}/requested": {
        "after": {
          "success": {
            "header": "Wydarzenie zakończone",
            "message": "Wydarzenie zostało pomyślnie zakończone"
          }
        }
      }
    },
    "POST": {
      "/api/v1/customer": {
        "after": {
          "success": {
            "header": "Utworzono klienta",
            "message": "Klient został utworzony."
          }
