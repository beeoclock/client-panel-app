	"http": {
		"DELETE": {
			"/api/v1/customer/{id}": {
				"before": {
					"accept": {
						"header": "Deleting a customer",
						"message": "Are you sure you want to remove the customer?"
					}
				}
			},
			"/api/v1/service/{id}": {
				"before": {
					"accept": {
						"header": "Deleting a service",
						"message": "Are you sure you want to remove the service?"
					}
				}
			}
		},
		"PATCH": {
			"/api/v1/event/{id}/booked": {
				"after": {
					"success": {
						"header": "Event booked",
						"message": "Event was successfully booked."
					}
				}
			},
			"/api/v1/event/{id}/cancelled": {
				"after": {
					"success": {
						"header": "Event cancelled",
						"message": "Event was successfully cancelled."
					}
				}
			},
			"/api/v1/event/{id}/done": {
				"after": {
					"success": {
						"header": "Event done",
						"message": "Event was successfully done."
					}
				}
			},
			"/api/v1/event/{id}/requested": {
				"after": {
					"success": {
						"header": "Event requested",
						"message": "Event was successfully requested."
					}
				}
			}
		},
		"POST": {
			"/api/v1/customer": {
				"after": {
					"success": {
						"header": "Customer created",
						"message": "Customer was successfully created."
					}
				}
			},
			"/api/v1/event": {
				"after": {
					"success": {
						"header": "Event created",
						"message": "Event was successfully created."
					}
				}
			},
			"/api/v1/service": {
				"after": {
					"success": {
						"header": "Service created",
						"message": "Service was successfully created."
					}
				}
			}
		},
		"PUT": {
			"/api/v1/business-profile": {
				"after": {
					"success": {
						"header": "Business profile",
						"message": "Business profile was successfully updated."
					}
				}
			},
			"/api/v1/customer/{id}": {
				"after": {
					"success": {
						"header": "Customer updated",
						"message": "Customer was successfully updated."
					}
				}
			},
			"/api/v1/event/{id}": {
				"after": {
					"success": {
						"header": "Event updated",
						"message": "Event was successfully updated."
					}
				}
			},
			"/api/v1/service/{id}": {
				"after": {
					"success": {
						"header": "Service updated",
						"message": "Service was successfully updated."
					}
				}
			}
		},
