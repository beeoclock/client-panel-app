{
  "name": "client-panel-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:local": "ng serve --configuration local",
    "start:emulator": "ng serve --configuration emulator",
    "start:emulator:authorization": "ng serve --configuration emulator-authorization",
    "start:local:external": "ng serve --configuration local --host 0.0.0.0 --disable-host-check",
    "start:external": "ng serve --host 0.0.0.0 --disable-host-check",
    "start:external:emulator": "ng serve --host 0.0.0.0 --disable-host-check --configuration emulator",
    "start:external:emulator:authorization": "ng serve --host 0.0.0.0 --disable-host-check --configuration emulator-authorization",
    "build": "ng build",
    "build:develop": "ng build --configuration develop",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "lint": "ng lint"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/setup-jest.ts"
    ],
    "moduleNameMapper": {
      "@src/(.*)": "<rootDir>/src/$1",
      "@environment/(.*)": "<rootDir>/src/environment/$1",
      "@asset/(.*)": "<rootDir>/src/asset/$1",
      "@scss/(.*)": "<rootDir>/src/asset/scss/$1",
      "@icon/(.*)": "<rootDir>/src/asset/icon/$1",
      "@app/(.*)": "<rootDir>/src/app/$1",
      "@module/(.*)": "<rootDir>/src/module/$1",
      "@utility/(.*)": "<rootDir>/src/module/utility/$1",
      "@identity/(.*)": "<rootDir>/src/module/identity/$1",
      "@client/(.*)": "<rootDir>/src/module/client/$1",
      "@customer/(.*)": "<rootDir>/src/module/customer/$1",
      "@event/(.*)": "<rootDir>/src/module/event/$1",
      "@service/(.*)": "<rootDir>/src/module/service/$1",
      "@member/(.*)": "<rootDir>/src/module/member/$1"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "17.0.4",
    "@angular/cdk": "17.0.1",
    "@angular/common": "17.0.4",
    "@angular/compiler": "17.0.4",
    "@angular/core": "17.0.4",
    "@angular/fire": "16.0.0",
    "@angular/forms": "17.0.4",
    "@angular/platform-browser": "17.0.4",
    "@angular/platform-browser-dynamic": "17.0.4",
    "@angular/router": "17.0.4",
    "@angular/service-worker": "17.0.4",
    "@ionic/angular": "7.5.6",
    "@ng-select/ng-select": "12.0.4",
    "@ngx-translate/core": "15.0.0",
    "@ngx-translate/http-loader": "8.0.0",
    "@ngxs/store": "3.8.1",
    "@p4ck493/ts-type-guard": "1.0.3",
    "bootstrap-icons": "1.11.1",
    "flowbite": "2.2.0",
    "hash-sum": "2.0.0",
    "humanize-duration": "3.31.0",
    "luxon": "3.4.4",
    "ng-event-bus": "5.2.0",
    "ngx-indexed-db": "16.0.0",
    "ngx-logger": "5.0.12",
    "ngx-mask": "17.0.0",
    "rxjs": "7.8.1",
    "thiis": "2.0.4",
    "tslib": "2.6.2",
    "typescript-debounce-decorator": "0.0.18",
    "uuid": "9.0.1",
    "zone.js": "0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "17.0.2",
    "@angular-eslint/builder": "17.1.0",
    "@angular-eslint/eslint-plugin": "17.1.0",
    "@angular-eslint/eslint-plugin-template": "17.1.0",
    "@angular-eslint/schematics": "17.1.0",
    "@angular-eslint/template-parser": "17.1.0",
    "@angular/cli": "17.0.2",
    "@angular/compiler-cli": "17.0.4",
    "@babel/core": "7.23.3",
    "@compodoc/compodoc": "1.1.22",
    "@ngxs/devtools-plugin": "3.8.1",
    "@tailwindcss/typography": "^0.5.10",
    "@testing-library/angular": "14.5.0",
    "@types/hash-sum": "1.0.2",
    "@types/humanize-duration": "3.27.3",
    "@types/jest": "29.5.9",
    "@types/luxon": "3.3.5",
    "@types/uuid": "9.0.7",
    "@typescript-eslint/eslint-plugin": "6.12.0",
    "@typescript-eslint/parser": "6.12.0",
    "autoprefixer": "10.4.16",
    "babel-loader": "9.1.3",
    "eslint": "8.54.0",
    "jest": "29.7.0",
    "jest-preset-angular": "13.1.4",
    "tailwindcss": "3.3.5",
    "typescript": "5.2.2"
  }
}
