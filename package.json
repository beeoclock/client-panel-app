{
	"name": "client-panel-app",
	"version": "0.0.0",
	"scripts": {
		"ng": "ng",
		"start": "ng serve --host 0.0.0.0 --disable-host-check --configuration external",
		"start:local": "ng serve --configuration local",
		"start:local_production": "ng serve --configuration local_production",
		"start:production": "ng serve --configuration production",
		"start:emulator": "ng serve --configuration emulator",
		"start:emulator:authorization": "ng serve --configuration emulator-authorization",
		"start:local:external": "ng serve --configuration local --host 0.0.0.0 --disable-host-check",
		"start:production:external": "ng serve --configuration production --host 0.0.0.0 --disable-host-check",
		"start:external": "ng serve --host 0.0.0.0 --disable-host-check --configuration external",
		"start:external:emulator": "ng serve --host 0.0.0.0 --disable-host-check --configuration emulator",
		"start:external:emulator:authorization": "ng serve --host 0.0.0.0 --disable-host-check --configuration emulator-authorization",
		"build": "ng build",
		"build:develop": "ng build --configuration develop",
		"build:pre-prod": "ng build --configuration pre-prod",
		"watch": "ng build --watch --configuration development",
		"test": "jest",
		"docs:json": "compodoc -p ./tsconfig.json -e json -d .",
		"docs": "compodoc -p tsconfig.json -s",
		"lint": "ng lint",
		"prepare": "ts-patch install && typia patch",
		"start:ssl": "ng serve --ssl true --ssl-key ./_certifications/localhost.key --ssl-cert ./_certifications/localhost.crt --host 192.168.0.234 --port 4200"
	},
	"jest": {
		"preset": "jest-preset-angular",
		"setupFilesAfterEnv": [
			"<rootDir>/setup-jest.ts"
		],
		"moduleNameMapper": {
			"@src/(.*)": "<rootDir>/src/$1",
			"@environment/(.*)": "<rootDir>/src/environment/$1",
			"@asset/(.*)": "<rootDir>/src/asset/$1",
			"@scss/(.*)": "<rootDir>/src/asset/scss/$1",
			"@icon/(.*)": "<rootDir>/src/asset/icon/$1",
			"@app/(.*)": "<rootDir>/src/app/$1",
			"@module/(.*)": "<rootDir>/src/module/$1",
			"@shared/(.*)": "<rootDir>/src/shared/$1",
			"@tenant/(.*)": "<rootDir>/src/tenant/$1",
			"@core/(.*)": "<rootDir>/src/core/$1",
			"@shared-kernel/(.*)": "<rootDir>/src/shared-kernel/$1",
			"@identity/(.*)": "<rootDir>/src/identity/$1"
		}
	},
	"private": true,
	"dependencies": {
		"@amcharts/amcharts5": "5.13.6",
		"@angular-architects/ngrx-toolkit": "20.3.0",
		"@angular/animations": "20.2.3",
		"@angular/cdk": "20.2.1",
		"@angular/common": "20.2.3",
		"@angular/compiler": "20.2.3",
		"@angular/core": "20.2.3",
		"@angular/fire": "20.0.1",
		"@angular/forms": "20.2.3",
		"@angular/platform-browser": "20.2.3",
		"@angular/platform-browser-dynamic": "20.2.3",
		"@angular/router": "20.2.3",
		"@angular/service-worker": "20.2.3",
		"@ionic/angular": "8.7.3",
		"@ng-icons/core": "32.1.0",
		"@ng-select/ng-select": "20.1.3",
		"@ngrx/signals": "20.0.1",
		"@ngx-translate/core": "17.0.0",
		"@ngx-translate/http-loader": "17.0.0",
		"@ngxs-labs/dispatch-decorator": "5.0.0",
		"@ngxs-labs/select-snapshot": "5.0.0",
		"@ngxs/router-plugin": "20.1.0",
		"@ngxs/store": "20.1.0",
		"@p4ck493/checker": "0.0.2",
		"@p4ck493/ts-type-guard": "1.0.3",
		"@sentry/angular": "10.8.0",
		"@swimlane/ngx-datatable": "github:beeoclock/npm-package-ngx-datatable#dist",
		"bootstrap-icons": "1.13.1",
		"bson-objectid": "2.0.4",
		"dexie": "4.2.0",
		"flat": "6.0.1",
		"flowbite": "3.1.2",
		"hammerjs": "2.0.8",
		"hash-sum": "2.0.0",
		"humanize-duration": "3.33.0",
		"intl-tel-input": "25.8.3",
