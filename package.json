{
  "name": "client-panel-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:local": "ng serve --configuration local",
    "start:emulator": "ng serve --configuration emulator",
    "start:emulator:authorization": "ng serve --configuration emulator-authorization",
    "start:local:external": "ng serve --configuration local --host 0.0.0.0 --disable-host-check",
    "start:external": "ng serve --host 0.0.0.0 --disable-host-check",
    "start:external:emulator": "ng serve --host 0.0.0.0 --disable-host-check --configuration emulator",
    "start:external:emulator:authorization": "ng serve --host 0.0.0.0 --disable-host-check --configuration emulator-authorization",
    "build": "ng build",
    "build:develop": "ng build --configuration develop",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "lint": "ng lint",
    "#COMMENTED# postbuild": "node environments/postbuild.js"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/setup-jest.ts"
    ],
    "moduleNameMapper": {
      "@src/(.*)": "<rootDir>/src/$1",
      "@environment/(.*)": "<rootDir>/src/environment/$1",
      "@assets/(.*)": "<rootDir>/src/assets/$1",
      "@scss/(.*)": "<rootDir>/src/assets/scss/$1",
      "@icon/(.*)": "<rootDir>/src/assets/icon/$1",
      "@app/(.*)": "<rootDir>/src/app/$1",
      "@module/(.*)": "<rootDir>/src/module/$1",
      "@utility/(.*)": "<rootDir>/src/module/utility/$1",
      "@identity/(.*)": "<rootDir>/src/module/identity/$1",
      "@client/(.*)": "<rootDir>/src/module/client/$1",
      "@customer/(.*)": "<rootDir>/src/module/customer/$1",
      "@event/(.*)": "<rootDir>/src/module/event/$1",
      "@service/(.*)": "<rootDir>/src/module/service/$1",
      "@member/(.*)": "<rootDir>/src/module/member/$1",
      "@user/(.*)": "<rootDir>/src/module/user/$1"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.0.0",
    "@angular/cdk": "^15.2.9",
    "@angular/common": "^15.0.0",
    "@angular/compiler": "^15.0.0",
    "@angular/core": "^15.0.0",
    "@angular/fire": "^7.5.0",
    "@angular/forms": "^15.0.0",
    "@angular/platform-browser": "^15.0.0",
    "@angular/platform-browser-dynamic": "^15.0.0",
    "@angular/router": "^15.0.0",
    "@ionic/angular": "7.3.3",
    "@ng-select/ng-select": "10.0.4",
    "@ngx-translate/core": "15.0.0",
    "@ngx-translate/http-loader": "8.0.0",
    "@ngxs/store": "3.8.1",
    "@p4ck493/ts-type-guard": "1.0.1",
    "bootstrap-icons": "1.10.5",
    "flowbite": "1.8.1",
    "hash-sum": "2.0.0",
    "humanize-duration": "3.29.0",
    "luxon": "3.4.3",
    "ng-event-bus": "5.2.0",
    "ngx-indexed-db": "^16.0.0",
    "ngx-mask": "^15.1.5",
    "rxjs": "7.8.1",
    "thiis": "1.0.2",
    "tslib": "2.6.2",
    "typescript-debounce-decorator": "0.0.18",
    "uuid": "9.0.0",
    "zone.js": "0.13.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.0.4",
    "@angular-eslint/builder": "~15.2.1",
    "@angular-eslint/eslint-plugin": "~15.2.1",
    "@angular-eslint/eslint-plugin-template": "~15.2.1",
    "@angular-eslint/schematics": "~15.2.1",
    "@angular-eslint/template-parser": "~15.2.1",
    "@angular/cli": "~15.2.2",
    "@angular/compiler-cli": "^15.0.0",
		"@types/hash-sum": "1.0.0",
		"@types/luxon": "3.3.2",
    "@babel/core": "7.22.17",
    "@compodoc/compodoc": "1.1.21",
    "@ngxs/devtools-plugin": "3.8.1",
    "@testing-library/angular": "14.3.0",
    "@types/humanize-duration": "3.27.1",
    "@types/jest": "29.5.4",
    "@types/uuid": "9.0.3",
    "@typescript-eslint/eslint-plugin": "6.7.0",
    "@typescript-eslint/parser": "6.7.0",
    "autoprefixer": "10.4.15",
    "babel-loader": "9.1.3",
    "eslint": "8.49.0",
    "jest": "29.7.0",
    "jest-preset-angular": "13.1.1",
    "tailwindcss": "3.3.3",
    "typescript": "4.9.5"
  }
}
