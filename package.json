{
	"name": "client-panel-app",
	"version": "0.0.0",
	"scripts": {
		"ng": "ng",
		"start": "ng serve",
		"start:local": "ng serve --configuration local",
		"start:local_production": "ng serve --configuration local_production",
		"start:production": "ng serve --configuration production",
		"start:emulator": "ng serve --configuration emulator",
		"start:emulator:authorization": "ng serve --configuration emulator-authorization",
		"start:local:external": "ng serve --configuration local --host 0.0.0.0 --disable-host-check",
		"start:production:external": "ng serve --configuration production --host 0.0.0.0 --disable-host-check",
		"start:external": "ng serve --host 0.0.0.0 --disable-host-check",
		"start:external:emulator": "ng serve --host 0.0.0.0 --disable-host-check --configuration emulator",
		"start:external:emulator:authorization": "ng serve --host 0.0.0.0 --disable-host-check --configuration emulator-authorization",
		"build": "ng build",
		"build:develop": "ng build --configuration develop",
		"watch": "ng build --watch --configuration development",
		"test": "jest",
		"docs:json": "compodoc -p ./tsconfig.json -e json -d .",
		"docs": "compodoc -p tsconfig.json -s",
		"lint": "ng lint",
		"prepare": "ts-patch install && typia patch"
	},
	"jest": {
		"preset": "jest-preset-angular",
		"setupFilesAfterEnv": [
			"<rootDir>/setup-jest.ts"
		],
		"moduleNameMapper": {
			"@src/(.*)": "<rootDir>/src/$1",
			"@environment/(.*)": "<rootDir>/src/environment/$1",
			"@asset/(.*)": "<rootDir>/src/asset/$1",
			"@scss/(.*)": "<rootDir>/src/asset/scss/$1",
			"@icon/(.*)": "<rootDir>/src/asset/icon/$1",
			"@app/(.*)": "<rootDir>/src/app/$1",
			"@module/(.*)": "<rootDir>/src/module/$1",
			"@utility/(.*)": "<rootDir>/src/module/utility/$1",
			"@identity/(.*)": "<rootDir>/src/module/identity/$1",
			"@client/(.*)": "<rootDir>/src/module/client/$1",
			"@customer/(.*)": "<rootDir>/src/module/customer/$1",
			"@event/(.*)": "<rootDir>/src/module/event/$1",
			"@service/(.*)": "<rootDir>/src/module/service/$1",
			"@member/(.*)": "<rootDir>/src/module/member/$1",
			"@core/(.*)": "<rootDir>/src/core/$1"
		}
	},
	"private": true,
	"dependencies": {
		"@amcharts/amcharts5": "5.10.10",
		"@angular-architects/ngrx-toolkit": "19.0.2",
		"@angular/animations": "19.0.6",
		"@angular/cdk": "19.0.5",
		"@angular/common": "19.0.6",
		"@angular/compiler": "19.0.6",
		"@angular/core": "19.0.6",
		"@angular/fire": "19.0.0-rc.5",
		"@angular/forms": "19.0.6",
		"@angular/platform-browser": "19.0.6",
		"@angular/platform-browser-dynamic": "19.0.6",
		"@angular/router": "19.0.6",
		"@angular/service-worker": "19.0.6",
		"@ionic/angular": "8.4.1",
		"@ng-icons/core": "30.2.0",
		"@ng-select/ng-select": "14.1.0",
		"@ngrx/signals": "19.0.0",
		"@ngx-translate/core": "16.0.4",
		"@ngx-translate/http-loader": "16.0.1",
		"@ngxs-labs/dispatch-decorator": "5.0.0",
		"@ngxs-labs/select-snapshot": "5.0.0",
		"@ngxs/router-plugin": "19.0.0",
		"@ngxs/store": "19.0.0",
		"@p4ck493/checker": "0.0.2",
		"@p4ck493/ts-type-guard": "1.0.3",
		"@sentry/angular": "8.48.0",
		"@signaldb/angular": "1.0.4",
		"@signaldb/core": "1.2.3",
		"@signaldb/indexeddb": "1.0.0",
		"@signaldb/sync": "1.1.1",
		"bootstrap-icons": "1.11.3",
		"bson-objectid": "2.0.4",
		"dexie": "4.0.11",
		"flowbite": "2.5.2",
		"hammerjs": "2.0.8",
		"hash-sum": "2.0.0",
		"humanize-duration": "3.32.1",
		"intl-tel-input": "25.2.1",
		"luxon": "3.5.0",
