{
	"name": "client-panel-app",
	"version": "0.0.0",
	"scripts": {
		"ng": "ng",
		"start": "ng serve",
		"start:local": "ng serve --configuration local",
		"start:local_production": "ng serve --configuration local_production",
		"start:production": "ng serve --configuration production",
		"start:emulator": "ng serve --configuration emulator",
		"start:emulator:authorization": "ng serve --configuration emulator-authorization",
		"start:local:external": "ng serve --configuration local --host 0.0.0.0 --disable-host-check",
		"start:production:external": "ng serve --configuration production --host 0.0.0.0 --disable-host-check",
		"start:external": "ng serve --host 0.0.0.0 --disable-host-check",
		"start:external:emulator": "ng serve --host 0.0.0.0 --disable-host-check --configuration emulator",
		"start:external:emulator:authorization": "ng serve --host 0.0.0.0 --disable-host-check --configuration emulator-authorization",
		"build": "ng build",
		"build:develop": "ng build --configuration develop",
		"watch": "ng build --watch --configuration development",
		"test": "jest",
		"docs:json": "compodoc -p ./tsconfig.json -e json -d .",
		"docs": "compodoc -p tsconfig.json -s",
		"lint": "ng lint",
		"prepare": "ts-patch install && typia patch"
	},
	"jest": {
		"preset": "jest-preset-angular",
		"setupFilesAfterEnv": [
			"<rootDir>/setup-jest.ts"
		],
		"moduleNameMapper": {
			"@src/(.*)": "<rootDir>/src/$1",
			"@environment/(.*)": "<rootDir>/src/environment/$1",
			"@asset/(.*)": "<rootDir>/src/asset/$1",
			"@scss/(.*)": "<rootDir>/src/asset/scss/$1",
			"@icon/(.*)": "<rootDir>/src/asset/icon/$1",
			"@app/(.*)": "<rootDir>/src/app/$1",
			"@module/(.*)": "<rootDir>/src/module/$1",
			"@utility/(.*)": "<rootDir>/src/module/utility/$1",
			"@identity/(.*)": "<rootDir>/src/module/identity/$1",
			"@client/(.*)": "<rootDir>/src/module/client/$1",
			"@customer/(.*)": "<rootDir>/src/module/customer/$1",
			"@event/(.*)": "<rootDir>/src/module/event/$1",
			"@service/(.*)": "<rootDir>/src/module/service/$1",
			"@member/(.*)": "<rootDir>/src/module/member/$1"
		}
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "18.2.8",
		"@angular/cdk": "18.2.8",
		"@angular/common": "18.2.8",
		"@angular/compiler": "18.2.8",
		"@angular/core": "18.2.8",
		"@angular/fire": "18.0.1",
		"@angular/forms": "18.2.8",
		"@angular/platform-browser": "18.2.8",
		"@angular/platform-browser-dynamic": "18.2.8",
		"@angular/router": "18.2.8",
		"@angular/service-worker": "18.2.8",
		"@ionic/angular": "8.3.2",
		"@ng-icons/core": "29.5.1",
		"@ng-select/ng-select": "13.9.1",
		"@ngx-translate/core": "15.0.0",
		"@ngx-translate/http-loader": "8.0.0",
		"@ngxs-labs/dispatch-decorator": "5.0.0",
		"@ngxs-labs/select-snapshot": "5.0.0",
		"@ngxs/store": "18.1.1",
		"@p4ck493/checker": "0.0.2",
		"@p4ck493/ts-type-guard": "1.0.3",
		"bootstrap-icons": "1.11.3",
		"bson-objectid": "2.0.4",
		"flowbite": "2.5.2",
		"hammerjs": "2.0.8",
		"hash-sum": "2.0.0",
		"humanize-duration": "3.32.1",
		"intl-tel-input": "24.6.0",
		"luxon": "3.5.0",
		"ng-event-bus": "7.0.1",
		"ngx-logger": "5.0.12",
		"ngx-mask": "18.0.0",
		"rxjs": "7.8.1",
		"tslib": "2.8.0",
		"typescript-debounce-decorator": "0.0.18",
		"typia": "6.11.2",
		"uuid": "10.0.0",
		"zone.js": "0.14.10"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "18.2.8",
		"@angular-eslint/builder": "18.3.1",
		"@angular-eslint/eslint-plugin": "18.3.1",
		"@angular-eslint/eslint-plugin-template": "18.3.1",
		"@angular-eslint/schematics": "18.3.1",
		"@angular-eslint/template-parser": "18.3.1",
		"@angular/cli": "18.2.8",
		"@angular/compiler-cli": "18.2.8",
		"@babel/core": "7.25.8",
		"@compodoc/compodoc": "1.1.26",
		"@ng-icons/bootstrap-icons": "29.5.1",
		"@ng-icons/material-icons": "29.5.1",
		"@ng-icons/remixicon": "29.5.1",
		"@ng-icons/tdesign-icons": "29.5.1",
		"@ngxs/devtools-plugin": "18.1.1",
		"@tailwindcss/aspect-ratio": "0.4.2",
		"@tailwindcss/forms": "0.5.9",
		"@tailwindcss/typography": "0.5.15",
		"@testing-library/angular": "17.3.1",
		"@types/hammerjs": "2.0.46",
		"@types/hash-sum": "1.0.2",
		"@types/humanize-duration": "3.27.4",
		"@types/jest": "29.5.13",
		"@types/luxon": "3.4.2",
		"@types/uuid": "10.0.0",
		"@typescript-eslint/eslint-plugin": "8.9.0",
		"@typescript-eslint/parser": "8.9.0",
		"autoprefixer": "10.4.20",
		"babel-loader": "9.2.1",
		"eslint": "9.12.0",
		"jest": "29.7.0",
		"jest-preset-angular": "14.2.4",
		"tailwindcss": "3.4.14",
		"ts-node": "10.9.2",
		"ts-patch": "3.2.1",
		"typescript": "5.5.4"
	}
}
