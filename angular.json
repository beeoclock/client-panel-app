{
	"$schema": "./node_modules/@angular/cli/lib/config/schema.json",
	"version": 1,
	"newProjectRoot": "projects",
	"projects": {
		"client-panel-app": {
			"projectType": "application",
			"schematics": {
				"@schematics/angular:component": {
					"style": "scss"
				}
			},
			"root": "",
			"sourceRoot": "src",
			"prefix": "app",
			"architect": {
				"build": {
					"builder": "@angular-devkit/build-angular:browser",
					"options": {
						"allowedCommonJsDependencies": [
							"hash-sum",
							"typescript-debounce-decorator",
							"typia",
							"bson-objectid"
						],
						"outputPath": "dist",
						"index": "src/index.html",
						"main": "src/main.ts",
						"polyfills": [
							"zone.js"
						],
						"tsConfig": "tsconfig.app.json",
						"inlineStyleLanguage": "scss",
						"assets": [
							"src/favicon.ico",
							"src/asset",
							"src/manifest.webmanifest"
						],
						"styles": [
							"src/styles.scss"
						],
						"serviceWorker": true,
						"ngswConfigPath": "ngsw-config.json"
					},
					"configurations": {
						"production": {
							"assets": [
								"src/favicon.ico",
								"src/asset",
								"src/manifest.webmanifest",
								{
									"glob": "firebase-messaging-sw.js",
									"input": "environment/production/firebase/",
									"output": "./"
								},
								{
									"glob": "robots.txt",
									"input": "environment/production/",
									"output": "./"
								}
							],
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "2mb",
									"maximumError": "5mb"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "2kb",
									"maximumError": "4kb"
								}
							],
							"fileReplacements": [
								{
									"replace": "src/environment/environment.ts",
									"with": "src/environment/environment.prod.ts"
								}
							],
							"outputHashing": "all"
						},
						"develop": {
							"assets": [
								"src/favicon.ico",
								"src/asset",
								"src/manifest.webmanifest",
								{
									"glob": "firebase-messaging-sw.js",
									"input": "environment/develop/firebase",
									"output": "./"
								},
								{
									"glob": "robots.txt",
									"input": "environment/develop/",
									"output": "./"
								}
							],
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "2mb",
									"maximumError": "5mb"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "2kb",
									"maximumError": "4kb"
								}
							],
							"fileReplacements": [
								{
									"replace": "src/environment/environment.ts",
									"with": "src/environment/environment.develop.ts"
								}
							],
							"outputHashing": "all"
						},
						"development": {
							"buildOptimizer": false,
							"optimization": false,
							"vendorChunk": true,
							"extractLicenses": false,
							"sourceMap": true,
							"namedChunks": true
						},
						"local": {
							"buildOptimizer": false,
							"optimization": false,
							"vendorChunk": true,
							"extractLicenses": false,
							"sourceMap": true,
							"namedChunks": true,
							"fileReplacements": [
								{
									"replace": "src/environment/environment.ts",
									"with": "src/environment/environment.local.ts"
								}
							],
							"outputHashing": "all"
						},
						"local_production": {
							"buildOptimizer": false,
							"optimization": false,
							"vendorChunk": true,
							"extractLicenses": false,
							"sourceMap": true,
							"namedChunks": true,
							"fileReplacements": [
								{
									"replace": "src/environment/environment.ts",
									"with": "src/environment/environment.local_production.ts"
								}
							],
							"outputHashing": "all"
						},
						"emulator": {
							"buildOptimizer": false,
							"optimization": false,
							"vendorChunk": true,
							"extractLicenses": false,
							"sourceMap": true,
							"namedChunks": true,
							"fileReplacements": [
								{
									"replace": "src/environment/environment.ts",
									"with": "src/environment/environment.emulator.ts"
								}
							],
							"outputHashing": "all"
						},
						"emulator-authorization": {
							"buildOptimizer": false,
							"optimization": false,
							"vendorChunk": true,
							"extractLicenses": false,
							"sourceMap": true,
							"namedChunks": true,
							"fileReplacements": [
								{
									"replace": "src/environment/environment.ts",
									"with": "src/environment/environment.emulator.authorization.ts"
								}
							],
							"outputHashing": "all"
						}
					},
					"defaultConfiguration": "production"
				},
				"serve": {
					"builder": "@angular-devkit/build-angular:dev-server",
					"configurations": {
						"production": {
							"buildTarget": "client-panel-app:build:production"
						},
						"develop": {
							"buildTarget": "client-panel-app:build:develop"
						},
						"development": {
							"buildTarget": "client-panel-app:build:development"
						},
						"emulator": {
							"buildTarget": "client-panel-app:build:emulator"
						},
						"local": {
							"buildTarget": "client-panel-app:build:local"
						},
						"local_production": {
							"buildTarget": "client-panel-app:build:local_production"
						},
						"emulator-authorization": {
							"buildTarget": "client-panel-app:build:emulator-authorization"
						}
					},
					"defaultConfiguration": "development"
				},
				"extract-i18n": {
					"builder": "@angular-devkit/build-angular:extract-i18n",
					"options": {
						"buildTarget": "client-panel-app:build"
					}
				},
				"lint": {
					"builder": "@angular-eslint/builder:lint",
					"options": {
						"lintFilePatterns": [
							"src/**/*.ts",
							"src/**/*.html"
						]
					}
				}
			}
		}
	},
	"cli": {
		"schematicCollections": [
			"@angular-eslint/schematics"
		],
		"analytics": false
	}
}
